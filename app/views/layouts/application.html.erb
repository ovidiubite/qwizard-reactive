<!DOCTYPE html>
<html>
  <head>
    <title>Qwizard</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= action_cable_meta_tag %>

    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <turbo-frame id="flash">
      <%= render 'shared/flash' %>
    </turbo-frame>

    <%= yield %>
  </body>
</html>
